{
  "name": "onestack-server",
  "version": "1.0.0",
  "main": "./src/index.js",
  "engines": {
    "node": "8.11.1"
  },
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "test": "babel-tape-runner",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/index.js",
    "start-dev": "nodemon --exec babel-node ./src/index.js",
    "db-init": "export NODE_ENV=development && createdbjs -h db --password=$POSTGRES_PASSWORD $POSTGRES_DB && knex migrate:latest && knex seed:run",
    "db-init-test": "export NODE_ENV=test && createdbjs -h db ${POSTGRES_DB}-test && knex migrate:latest && knex seed:run",
    "db-drop": "dropdbjs -h db --password=$POSTGRES_PASSWORD $POSTGRES_DB"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brunoreis/catalogo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brunoreis/catalogo/issues"
  },
  "homepage": "https://github.com/brunoreis/catalogo#readme",
  "dependencies": {
    "apollo-server": "^2.10.1",
    "apollo-server-express": "^2.10.1",
    "apollo-server-testing": "^2.10.1",
    "babel-core": "^7.0.0-bridge.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "graphql": "^14.6.0",
    "knex": "^0.16.5",
    "knex-stringcase": "^1.4.1",
    "moment": "^2.24.0",
    "node-uuid": "^1.4.8",
    "nodemailer": "^4.7.0",
    "nodemailer-sendgrid": "^1.0.3",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "password-reset": "^0.1.4",
    "pg": "^7.18.2",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "babel-tape-runner": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-tape": "^1.1.0",
    "graphql-tag": "^2.10.3",
    "nodemon": "^1.19.4",
    "tap-spec": "^5.0.0",
    "tape": "^4.13.0",
    "tape-async": "^2.3.0"
  },
  "description": ""
}
